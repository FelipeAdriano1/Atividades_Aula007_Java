package diferencaDatas;

import java.util.Calendar;
import java.util.Date;

public class DiferencaDatsa {

	public static void main(String[] args) {
	
	        Calendar cal1 = Calendar.getInstance();
	        Calendar cal2 = Calendar.getInstance();
	        
	        cal1.set(2024, Calendar.JULY, 10);
	        cal2.set(2024, Calendar.JULY, 20);

	        long difMilisegundos = cal1.getTimeInMillis() - cal2.getTimeInMillis();
	        long difDias = difMilisegundos / (24 * 60 * 60 * 1000); 
	        
	        System.out.println(difDias + " de diferença da data 1 até a data 2.");
	}
}
