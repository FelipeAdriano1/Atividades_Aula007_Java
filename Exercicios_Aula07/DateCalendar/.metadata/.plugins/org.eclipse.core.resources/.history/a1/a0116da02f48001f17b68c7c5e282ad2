package diferencaDatas;

import java.util.Calendar;
import java.util.Date;

public class DiferencaDatsa {

	public static void main(String[] args) {
	
	        Calendar cal1 = Calendar.getInstance();
	        Calendar cal2 = Calendar.getInstance();
	        
	        cal1.set(2024, Calendar.JULY, 10);
	        cal2.set(2024, Calendar.JULY, 20);

	        Date date1 = cal1.getTime();
	        Date date2 = cal2.getTime();

	        long difMilsegundos = cal1.getTimeInMillis() - cal2.getTimeInMillis();
	        
	        System.out.println("Diferen√ßa em dias: " + cal1.getTime());
	}
}
